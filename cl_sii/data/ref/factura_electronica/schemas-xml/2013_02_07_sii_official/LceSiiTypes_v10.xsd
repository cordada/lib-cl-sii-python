				</xs:annotation>
			</xs:enumeration>
			<xs:enumeration value="37">
				<xs:annotation>
					<xs:documentation>IVA Retenido Hidrobiologicas</xs:documentation>
				</xs:annotation>
			</xs:enumeration>
			<xs:enumeration value="38">
				<xs:annotation>
					<xs:documentation>IVA Retenido Chatarra</xs:documentation>
				</xs:annotation>
			</xs:enumeration>
			<xs:enumeration value="39">
